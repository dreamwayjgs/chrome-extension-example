{
    "name": "geo info highlighter",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background2.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "getSource.js",
        "getSource2.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "getSource.js",
                "getSource2.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "background",
        "activeTab",
        "<all_urls>",
        "http://*/",
        "https://*/"
    ]
}